{
  "$schema": "https://openapi.vercel.sh/config/v2.json",
  "builds": [
    {
      "src": "webapp/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webapp/app.py"
    }
  ],
  "functions": {
    "webapp/app.py": {
      "runtime": "python3.9",
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
